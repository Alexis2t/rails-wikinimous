<%= render "navbar" %>
<div class="container">
  <h1 class="text-center mt-4 mb-5">Articles</h1>
  <% @articles.each do |article| %>
    <div class="border-bottom mb-4">
      <h3 class="text-center"><%= link_to article.title, article_path(article) %></h3>
      <p><%= article.content %></p>
    </div>
  <% end %>
</div>
